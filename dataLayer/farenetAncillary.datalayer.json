{
    "id": "/farenetAncillary.datalayer",
    "$schema": "http://json-schema.org/draft-06/schema#",
    "title": "FareNet Ancillary DataLayer",
    "description": "FareNet Ancillary DataLayer",
    "type": "object",
    "properties": {
        "currencyCode": {
            "description": "The currency (in 3-letter ISO 4217 format) of the price.",
            "type": "string",
            "example": "USD"
        },
        "property": {
            "allOf": [
                {
                    "$ref": "../types/property.schema.json"
                }
            ]
        },
        "carRentals": {
            "allOf": [
                {
                    "$ref": "../types/carRental_base.schema.json"
                }
            ]
        },
        "insurance": {
            "brand": {
                "description": "Travel insurance provider",
                "type": "string",
                "example": "Platinum Plan"
            },
            "name": {
                "description": "name",
                "type": "string",
                "example": "AIG Travel Guard"
            },
            "priceSpecification": {
                "$ref": "../types/priceSpecification_base.schema.json"
            }
        },
        "baggage": {
            "count": {
                "description": "Total number of baggages",
                "type": "number",
                "example": 2
            },
            "checkedBags": {
                "description": "checkedBags",
                "type": "number",
                "example": 1
            },
            "carryOnBags": {
                "description": "carryOnBags",
                "type": "number",
                "example": 1
            },
            "priceSpecification": {
                "$ref": "../types/priceSpecification_base.schema.json"
            },
            "prepaidInd": {
                "description": "When true, indicates the baggage is prepaid",
                "type": "boolean",
                "example": true
            }
        },
        "seat": {
            "count": {
                "description": "Max. number of passengers in car rental",
                "type": "number",
                "example": 4
            },
            "rowNumber": {
                "description": "Number assigned to a row in the aircraft, typically used to identify seat assignments.",
                "type": "string",
                "example": "16"
            },
            "cabinColumnId": {
                "description": "Identifier of the column (e.g. A, B, C, etc.)",
                "type": "string",
                "example": "A"
            },
            "priceSpecification": {
                "$ref": "../types/priceSpecification_base.schema.json"
            }
        },
        "page": {
            "description": "WebPage",
            "type": "object",
            "properties": {
                "languageIsoCode": {
                    "description": "Language ISO 639-1 code",
                    "type": "string",
                    "example": "en"
                },
                "siteEdition": {
                    "description": "Site edition combination of ISO codes for language and country",
                    "type": "string",
                    "example": "en-US"
                }
            }
        }
    },
    "required": [
        "priceSpecification",
        "journey",
        "page"
    ],
    "links": [
        {
            "title": "FareNet Ancillary DataLayer",
            "description": "FareNet Ancillary DataLayer",
            "rel": "self",
            "href": "/farenetAncillary.datalayer",
            "method": "GET",
            "targetSchema": {
                "rel": "self"
            }
        }
    ]
}