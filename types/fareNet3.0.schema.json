{
  "id": "/fareNet",
  "$schema": "http://json-schema.org/draft-06/schema#",
  "title": "FareNet3.0",
  "description": "fareNet 3.0 includes journey only",
  "type": "object",
  "properties": {
    "journey": {
      "description": "Journey",
      "type": "object",
      "properties": {
        "airline": {
          "$ref": "./airline_base.schema.json"
        },
        "fullRoute": {
          "description": "fullRoute",
          "type": "string",
          "example": "SFO>MIA>JFK|JFK>SFO"
        },
        "priceSpecification": {
          "description": "priceSpecification",
          "type": "object",
          "allOf": [
            {"$ref": "./priceSpecification_base.schema.json"},
          {
          "properties": {
            "additionalFeesAmount": {
              "description": "additionalFeesAmount",
              "type": "number",
              "example": 12
            },
            "currencyCode": {
              "description": "currency",
              "type": "string",
              "example": "USD"
            },
            "currencySymbol": {
              "description": "currencySymbol",
              "type": "string",
              "example": "$"
            },
            "pricingGroupId": {
              "description": "priceMarketId",
              "type": "string",
              "example": "USD-US"
            },
            "redemption": {
              "$ref": "./redemption_base.schema.json"
            }
          }
          }
        ]},
        "passengerDetails": {
          "$ref": "./passengerDetails.schema.json"
        },
        "passenger": {
          "$ref": "./passenger.schema.json"
        },
        "fareTensor": {
          "$ref": "./fareTensor.schema.json"
        },
        "outboundFlight": {
          "description": "outboundFlight",
          "type": "object",
          "properties": {
            "boundType": {
              "description": "OUTBOUND, INBOUND, MATRIX",
              "type": "string",
              "example": "OUTBOUND"
            },
            "route": {
              "description": "route",
              "type": "string",
              "example": "SFO>JFK"
            },
            "priceSpecification": {
              "description": "priceSpecification",
              "type": "object",
              "properties": {
                "priceBeforeTaxesAndFees": {
                  "description": "priceBeforeTaxesAndFees",
                  "type": "number",
                  "example": 750.12
                },
                "totalPrice": {
                  "description": "totalPrice",
                  "type": "number",
                  "example": 825.51
                },
                "taxAmount": {
                  "description": "taxAmount",
                  "type": "number",
                  "example": 75.39
                },
                "redemption": {
                  "$ref": "./redemption_base.schema.json"
                }
              }
            },
            "fareClassInput": {
              "description": "Name of the fare class given by airline in sentence case, e.g. Economy Basic",
              "type": "string",
              "example": "Economy Basic"
            },
            "fareClass": {
              "description": "Standardized fare class can only be ECONOMY, BUSINESS, FIRST",
              "type": "string",
              "example": "ECONOMY"
            },
            "departureAirportIataCode": {
              "description": "departureAirportIataCode",
              "type": "string",
              "example": "SFO"
            },
            "arrivalAirportIataCode": {
              "description": "arrivalAirportIataCode",
              "type": "string",
              "example": "JFK"
            },
            "departureDate": {
              "description": "Departure Date",
              "type": "string",
              "example": "2017-12-07"
            },
            "estimatedDuration": {
              "description": "Estimated Duration of the flight",
              "type": "string",
              "example": "PT9H45M"
            },
            "availableSeatsCount": {
              "description": "availableSeatsCount",
              "type": "string",
              "example": "9+"
            },
            "legs": {
              "$ref": "./legs_base.schema.json"
              }
            },
            "departureCity": {
              "$ref": "./departureCity_base.schema.json"
            },
            "departureCountry": {
              "$ref": "./country_base.schema.json"
            },
            "arrivalCity": {
              "$ref": "./arrivalCity_base.schema.json"
            },
            "arrivalCountry": {
              "$ref": "./country_base.schema.json"
            },
            "flightType": {
              "description": "flightType",
              "type": "string",
              "example": "DOMESTIC"
            },
            "routeType": {
              "description": "route type can be CONNECTING or DIRECT",
              "type": "string",
              "example": "CONNECTING"
            },
            "discount": {
              "$ref": "./discount.schema.json"
            },
            "additionalInfo": {
              "$ref": "./additionalInfo.schema.json"
            }
        },
        "inboundFlight": {
          "description": "inboundFlight",
          "type": "object",
          "properties": {
            "boundType": {
              "description": "OUTBOUND, INBOUND, MATRIX",
              "type": "string",
              "example": "INBOUND"
            },
            "route": {
              "description": "route",
              "type": "string",
              "example": "JFK>SFO"
            },
            "priceSpecification": {
              "description": "priceSpecification",
              "type": "object",
              "properties": {
                "priceBeforeTaxesAndFees": {
                  "description": "priceBeforeTaxesAndFees",
                  "type": "number",
                  "example": 750.12
                },
                "totalPrice": {
                  "description": "totalPrice",
                  "type": "number",
                  "example": 825.51
                },
                "taxAmount": {
                  "description": "taxAmount",
                  "type": "number",
                  "example": 75.39
                },
                "redemption": {
                  "$ref": "./redemption_base.schema.json"
                }
              }
            },
            "fareClassInput": {
              "description": "fareClassInput",
              "type": "string",
              "example": "Economy"
            },
            "fareClass": {
              "description": "fareClass",
              "type": "string",
              "example": "ECONOMY"
            },
            "departureAirportIataCode": {
              "description": "departureAirportIataCode",
              "type": "string",
              "example": "JFK"
            },
            "arrivalAirportIataCode": {
              "description": "arrivalAirportIataCode",
              "type": "string",
              "example": "SFO"
            },
            "departureDate": {
              "description": "Departure Date",
              "type": "string",
              "example": "2017-12-10"
            },
            "estimatedDuration": {
              "description": "estimatedDuration",
              "type": "string",
              "example": "PT9H45M"
            },
            "availableSeatsCount": {
              "description": "availableSeatsCount",
              "type": "string",
              "example": "9+"
            },
            "legs": {
              "description": "legs",
              "type": "array",
              "items": {
                "type": "object"
              }
            },
            "departureCity": {
              "$ref": "./departureCity_base.schema.json"
            },
            "departureCountry": {
              "$ref": "./country_base.schema.json"
            },
            "arrivalCity": {
              "$ref": "./arrivalCity_base.schema.json"
            },
            "arrivalCountry": {
              "$ref": "./country_base.schema.json"
            },
            "flightType": {
              "description": "flightType",
              "type": "string",
              "example": "DOMESTIC"
            },
            "routeType": {
              "description": "routeType",
              "type": "string",
              "example": "NONSTOP"
            },
            "discount": {
              "$ref": "./discount.schema.json"
            },
            "additionalInfo": {
              "$ref": "./additionalInfo.schema.json"
            }
          }
        },
        "departureDate": {
          "description": "departureDate",
          "type": "string",
          "example": "2017-12-07"
        },
        "returnDate": {
          "description": "returnDate",
          "type": "string",
          "example": "2017-12-10"
        },
        "journeyType": {
          "description": "ROUND_TRIP, ONE_WAY, or MULTI_CITY",
          "type": "string",
          "example": "ROUND_TRIP"
        },
        "isCodeShare": {
          "description": "isCodeShare",
          "type": "boolean",
          "example": true
        },
        "flightType": {
          "description": "DOMESTIC, INTERNATIONAL",
          "type": "string",
          "example": "DOMESTIC"
        },
        "segmentCount": {
          "description": "segmentCount",
          "type": "number",
          "example": 2
        },
        "flightCount": {
          "description": "flightCount",
          "type": "number",
          "example": 3
        },
        "discount": {
          "$ref": "./discount.schema.json"
        }
    },
    "page": {
      "$ref": "./page_base.schema.json"
    },
    "device": {
      "$ref": "./device_base.schema.json"
    },
    "metadata": {
      "$ref": "./metadata_base.schema.json"
    }
  }},
  "required": ["journey", "page", "device", "metadata"],
  "links": [
    {
      "title": "FareNet 3.0",
      "description": "FareNet 3.0",
      "rel": "self",
      "href": "/fareNet",
      "method": "GET",
      "targetSchema": {
        "rel": "self"
      }
    }
  ]
}