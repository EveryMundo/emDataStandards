{
  "id": "/cells",
  "$schema": "http://json-schema.org/draft-06/schema#",
  "title": "Cells",
  "type": "array",
  "items":{
    "title": "Cell",
    "type": "object",
    "allOf": [
      {"$ref": "./redemption_base.schema.json"}, 
    {
    "properties": {
        "price": {
          "description": "Price for the reservation or ticket",
          "type": "number",
          "example": 345.21
        },
        "boundType": {
          "description": "OUTBOUND, INBOUND, MATRIX",
          "type": "string",
          "example": "OUTBOUND"
        },
        "isSoldOut": {
          "description": "Whether price is sold out",
          "type": "boolean",
          "example": false
        },
        "outboundDate": {
          "description": "Outbound date",
          "type": "string",
          "example": "2017-03-06"
        },
        "inboundDate": {
          "description": "Inbound date",
          "type": "string",
          "example": null
        }
      }}]
  },
  "required": ["price", "boundType", "isSoldOut"],
  "links": [
    {
      "title": "Cells",
      "rel": "self",
      "href": "/cells",
      "method": "GET",
      "targetSchema": {
        "rel": "self"
      }
    }
  ]
}