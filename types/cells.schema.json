{
  "id": "/cells",
  "$schema": "http://json-schema.org/draft-06/schema#",
  "title": "Cells",
  "type": "array",
  "items":{
    "title": "Cell",
    "description": "Cells in the matrix",
    "type": "object",
    "properties": {
      "priceSpecification": {
        "description": "priceSpecification",
        "type": "object",
        "properties": {
          "totalPrice": {
            "description": "totalPrice",
            "type": "number",
            "example": 825.51
          },
          "redemption": {
            "$ref": "./redemption_base.schema.json"
          }
        }
      },
        "boundType": {
          "description": "OUTBOUND, INBOUND, MATRIX",
          "type": "string",
          "example": "MATRIX"
        },
        "isSoldOut": {
          "description": "Whether price is sold out",
          "type": "boolean",
          "example": false
        },
        "outboundDate": {
          "description": "Outbound date",
          "type": "string",
          "example": "2017-03-06"
        },
        "inboundDate": {
          "description": "Inbound date",
          "type": "string",
          "example": "2017-03-26"
        }
    }
  },
  "required": ["totalPrice", "boundType", "isSoldOut"],
  "links": [
    {
      "title": "Cells",
      "rel": "self",
      "href": "/cells",
      "method": "GET",
      "targetSchema": {
        "rel": "self"
      }
    }
  ]
}