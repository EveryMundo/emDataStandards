{
  "id": "/region",
  "$schema": "http://json-schema.org/draft-06/schema#",
  "title": "Region",
  "description": "Region",
  "type": "object",
  "properties": {
    "name": {
      "description": "The name of the region",
      "type": "string",
      "example": "North America"
    },
    "regionCode": {
      "description": "Any region M.49 standards code",
      "type": "string",
      "example": "003"
    },
    "geo": {
      "description": "geo",
      "type": "object",
      "properties": {
        "latitude": {
          "description": "The latitude of the region",
          "type": "number"
        },
        "longitude": {
          "description": "The longitude of the region",
          "type": "number"
        }
      },
      "example": {
        "@type": "Geocoordinates",
        "latitude": 37.42,
        "longitude": -108.32
      }
    },
    "images": {
      "description": "Array of image URLs for the region",
      "type": "array",
      "example": [
        "northamerica.com/image.jpg",
        "northamerica.com/image2.jpg"
      ]
    }
  },
  "required": ["name", "regionCode"],
  "links": [
    {
      "title": "Region",
      "description": "Region",
      "rel": "self",
      "href": "/region",
      "method": "GET",
      "targetSchema": {
        "rel": "self"
      }
    }
  ]
}