{
  "id": "/airport",
  "$schema": "http://json-schema.org/draft-06/schema#",
  "title": "Airport",
  "description": "Airport",
  "type": "object",
  "properties": {
    "name": {
      "description": "The name of the airport",
      "type": "string",
      "example": "Miami International Airport"
    },
    "iataCode": {
      "description": "IATA identifier for the airport",
      "type": "string",
      "example": "MIA"
    },
    "geo": {
      "description": "geo",
      "type": "object",
      "properties": {
        "latitude": {
          "description": "The latitude of the  airport",
          "type": "number"
        },
        "longitude": {
          "description": "The longitude of the airport",
          "type": "number"
        }
      },
      "example": {
        "@type": "Geocoordinates",
        "latitude": 37.42,
        "longitude": -108.32
      }
    },
    "images": {
      "description": "Array of image URLs for the airport",
      "type": "array",
      "example": [
        "mia.com/image.jpg",
        "mia.com/image2.jpg"
      ]
    }
  },
  "required": ["name", "iataCode"],
  "links": [
    {
      "title": "Airport",
      "description": "Airport",
      "rel": "self",
      "href": "/airport",
      "method": "GET",
      "targetSchema": {
        "rel": "self"
      }
    }
  ]
}