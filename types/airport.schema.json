{
  "id": "/airport",
  "$schema": "http://json-schema.org/draft-06/schema#",
  "title": "Airport",
  "description": "Airport",
  "type": "object",
  "properties": {
    "name": {
      "description": "The name of the airport",
      "type": "string"
    },
    "iataCode": {
      "description": "IATA identifier for the airport",
      "type": "string"
    },
    "geo": {
      "description": "geo",
      "type": "object",
      "properties": {
        "latitude": {
          "description": "The latitude of the  airport",
          "type": "number"
        },
        "longitude": {
          "description": "The longitude of the airport",
          "type": "number"
        }
      }
    },
    "image": {
      "description": "Image URL for the airport",
      "type": "string"
    },
    "images": {
      "description": "Array of image URLs for the airport",
      "type": "array"
    }
  },
  "required": []
}