{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "title": "PostalAddress",
  "description": "PostalAddress",
  "type": "object",
  "properties": {
    "street": {
      "description": "street",
      "type": "string",
      "example": "Murgstr. 5"
    },
    "cityName": {
      "description": "cityName",
      "type": "string",
      "example": "San Diego"
    },
    "regionCode": {
      "description": "regionCode",
      "type": "string",
      "example": "CA"
    },
    "postalCode": {
      "description": "postalCode",
      "type": "string",
      "example": "94043"
    },
    "countryCode": {
      "description": "countryCode",
      "type": "string",
      "example": "US"
    }
  },
  "required": []
}