{
  "id": "/postalAddress",
  "$schema": "http://json-schema.org/draft-06/schema#",
  "title": "PostalAddress",
  "description": "PostalAddress",
  "type": "object",
  "properties": {
    "street": {
      "description": "Street address name",
      "type": "string",
      "example": "Murgstr. 5"
    },
    "cityName": {
      "description": "city name",
      "type": "string",
      "example": "San Diego"
    },
    "regionCode": {
      "description": "State identifier code",
      "type": "string",
      "example": "CA"
    },
    "postalCode": {
      "description": "Postal code number",
      "type": "string",
      "example": "94043"
    },
    "countryCode": {
      "description": "Country ISO code",
      "type": "string",
      "example": "US"
    }
  },
  "required": []
}