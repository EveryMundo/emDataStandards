{
  "id": "/state",
  "$schema": "http://json-schema.org/draft-06/schema#",
  "title": "State",
  "description": "State",
  "type": "object",
  "properties": {
    "name": {
      "description": "The name of the state",
      "type": "string",
      "example": "Florida"
    },
    "stateCode": {
      "description": "ISO code for the state",
      "type": "string",
      "example": "FL"
    },
    "geo": {
      "description": "geo",
      "type": "object",
      "properties": {
        "latitude": {
          "description": "The latitude of the state",
          "type": "number"
        },
        "longitude": {
          "description": "The longitude of the state",
          "type": "number"
        }
      },
      "example": {
        "@type": "Geocoordinates",
        "latitude": 37.42,
        "longitude": -108.32
      }
    },
    "image": {
      "description": "Image URL for the state",
      "type": "string",
      "example": "florida.com/image.jpg"
    },
    "images": {
      "description": "Array of image URLs for the state",
      "type": "array",
      "example": [
        "florida.com/image.jpg",
        "florida.com/image2.jpg"
      ]
    }
  },
  "required": [],
  "links": [
    {
      "title": "State",
      "description": "State",
      "rel": "self",
      "href": "/state",
      "method": "GET",
      "targetSchema": {
        "rel": "self"
      }
    }
  ]
}