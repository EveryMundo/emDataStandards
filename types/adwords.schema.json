{
  "id": "/adwords",
  "$schema": "http://json-schema.org/draft-06/schema#",
  "title": "Adwords",
  "description": "Adwords",
  "type": "object",
  "properties": {
    "accountName": {
      "description": "Adwords account name",
      "type": "string"
    },
    "accountId": {
      "description": "AdWords account unique identifier",
      "type": "string"
    },
    "campaignName": {
      "description": "Adwords campaign name",
      "type": "string"
    },
    "campaignId": {
      "description": "Adwords campaign unique identifier",
      "type": "string"
    },
    "campaignType": {
      "description": "Adwords campaign type",
      "type": "string"
    },
    "specialCampaign": {
      "description": "Identifier of special campaigns (ie., campaign test suffix)",
      "type": "string"
    },
    "adgroupName": {
      "description": "Adwords ad group name",
      "type": "string"
    },
    "adgroupId": {
      "description": "Adwords ad group unique identifier",
      "type": "string"
    },
    "geoTarget": {
      "description": "Location identifier for marketing purposes",
      "type": "string"
    },
    "market": {
      "description": "Country ISO code of market",
      "type": "string"
    },
    "languageIsoCode": {
      "description": "Adwords account language ISO code",
      "type": "string"
    },
    "routeType": {
      "description": "NONSTOP, CONNECTING, CODESHARE",
      "type": "string"
    },
    "locationType": {
      "description": "Route location type regarding origin and destination",
      "type": "string"
    },
    "keywordGroup": {
      "description": "Adwords keyword group",
      "type": "string"
    },
    "keywordType": {
      "description": "Adwords keyword type",
      "type": "string"
    },
    "matchType": {
      "description": "Adwords keyword match type",
      "type": "string"
    },
    "audience": {
      "description": "Adwords audience",
      "type": "string"
    },
    "marketingNetwork": {
      "description": "Adwords network type",
      "type": "string"
    },
    "searchEngine": {
      "description": "GOOGLE, BING, YANDEX, YAHOO, BAIDU, NAVER, YAHOO JAPAN",
      "type": "string"
    },
    "inputKey": {
      "description": "Single field including account name, campaign name, adgroup name, etc., delimited with '&&'",
      "type": "string"
    }
  },
  "required": []
}