{
  "id": "/airline",
  "$schema": "http://json-schema.org/draft-06/schema#",
  "title": "Airline",
  "description": "Airline",
  "type": "object",
  "properties": {
    "name": {
      "description": "The name of the airline",
      "type": "string",
      "example": "EveryMundo"
    },
    "iataCode": {
      "description": "IATA identifier for the airline",
      "type": "string",
      "example": "EM"
    },
    "image": {
      "description": "Image URL for the airline",
      "type": "string",
      "example": "everymundo.com/image.jpg"
    },
    "images": {
      "description": "Array of multiple image URLs for the airline",
      "type": "array",
      "example": [
        "everymundo.com/image.jpg",
        "everymundo.com/image2.jpg"
      ]
    }
  },
  "required": [],
  "links": [
    {
      "title": "Airline",
      "description": "Airline",
      "rel": "self",
      "href": "/airline",
      "method": "GET",
      "targetSchema": {
        "rel": "self"
      }
    }
  ]
}