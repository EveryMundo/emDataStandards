{
  "id": "/city",
  "$schema": "http://json-schema.org/draft-06/schema#",
  "title": "City",
  "description": "City",
  "type": "object",
  "properties": {
    "name": {
      "description": "The name of the city",
      "type": "string",
      "example": "Miami"
    },
    "cityCode": {
      "description": "ISO code for the city",
      "type": "string",
      "example": "MIA"
    },
    "geo": {
      "description": "geo",
      "type": "object",
      "properties": {
        "latitude": {
          "description": "The latitude of the city",
          "type": "number"
        },
        "longitude": {
          "description": "The longitude of the city",
          "type": "number"
        }
      },
      "example": {
        "@type": "Geocoordinates",
        "latitude": 37.42,
        "longitude": -108.32
      }
    },
    "image": {
      "description": "Image URL for the city",
      "type": "string",
      "example": "miami.com/image.jpg"
    },
    "images": {
      "description": "Array of image URLs for the city",
      "type": "array",
      "example": [
        "miami.com/image.jpg",
        "miami.com/image2.jpg"
      ]
    }
  },
  "required": [],
  "links": [
    {
      "title": "City",
      "description": "City",
      "rel": "self",
      "href": "/city",
      "method": "GET",
      "targetSchema": {
        "rel": "self"
      }
    }
  ]
}