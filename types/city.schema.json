{
  "id": "/city",
  "$schema": "http://json-schema.org/draft-06/schema#",
  "title": "City",
  "description": "City",
  "type": "object",
  "allOf": [
    {"$ref": "./city_base.schema.json"},
  {
  "properties": {
    "geo": {
      "description": "geo",
      "type": "object",
      "properties": {
        "latitude": {
          "description": "The latitude of the city",
          "type": "number",
          "example": 37.42
        },
        "longitude": {
          "description": "The longitude of the city",
          "type": "number",
          "example": -108.32
        }
      }
    },
    "images": {
      "description": "Array of image URLs for the city",
      "type": "array",
      "example": [
        "miami.com/image.jpg",
        "miami.com/image2.jpg"
      ]
    }
  }}],
  "required": ["name", "cityCode"],
  "links": [
    {
      "title": "City",
      "description": "City",
      "rel": "self",
      "href": "/city",
      "method": "GET",
      "targetSchema": {
        "rel": "self"
      }
    }
  ]
}